{
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4,
    "editor.detectIndentation": false
  },
  "files.exclude": {
    "archive/**": true,
    "tests_archive/**": true,
    "kepler-koi-ml/**": true
  },
  "search.exclude": {
    "archive/**": true,
    "tests_archive/**": true,
    "kepler-koi-ml/**": true
},
"python.testing.unittestArgs": [
    "-v",
    "-s",
    ".",
    "-p",
    "*test.py"
],
"python.testing.pytestEnabled": false,
"python.testing.unittestEnabled": true
}
